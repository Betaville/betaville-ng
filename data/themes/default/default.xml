	<!-- Define the appearance of a Button widget -->
	<Button>
		<Appearance alignment="middle">
			<!-- define the spacing of this widget -->
			<Border all="0"/>
			<Margin left="5" right="5" bottom="5" top="0"/>
			<Padding all="5"/>

			<!-- define textrenderers -->
<!--		<BufferedTextRenderer ref="btr_default" key="default" />-->
<!--      	<BufferedTextRenderer ref="btr_pressed" key="fs_buttonPressed" /> -->
			<DirectTextRenderer ref="tr_font11" key="default" />
					
			<Switches>
         <FontStyleSwitch label="#Default" fontStyle="default" enabled="true" />
        <FontStyleSwitch label="Pressed#Pressed" fontStyle="fs_buttonPressed" enabled="false" /> 
				<TextColorSwitch label="#Default" enabled="true"><Color ref="c_buttonText" /></TextColorSwitch>
				<TextColorSwitch label="Active#Disabled" enabled="false"><Color ref="c_buttonTextDisabled" /></TextColorSwitch>
				<TextColorSwitch label="Pressed#Pressed" enabled="false"><Color ref="c_buttonTextPressed" /></TextColorSwitch>
			</Switches>
		
			<!-- define text styles -->
			<TextStyle key="default">
				<Style key="default" fontstyle="shadow"><Color ref="c_buttonText" /></Style>
			</TextStyle>

			
			<BackgroundDecorators clear="true">
				<GradientBackground label="#Default" enabled="true">
					<TopLeftColor ref="c_buttonBg0"/>
					<TopRightColor ref="c_buttonBg0"/>
					<BottomLeftColor ref="c_buttonBg1"/>
					<BottomRightColor ref="c_buttonBg1"/>
				</GradientBackground>

				<GradientBackground label="Focus#Focused" enabled="false">
					<TopLeftColor ref="c_buttonHoveredBg0"/>
					<TopRightColor ref="c_buttonHoveredBg0"/>
					<BottomLeftColor ref="c_buttonHoveredBg1"/>
					<BottomRightColor ref="c_buttonHoveredBg1"/>
				</GradientBackground>
				<GradientBackground label="Hover#Hovered" enabled="false">
					<TopLeftColor ref="c_buttonHoveredBg0"/>
					<TopRightColor ref="c_buttonHoveredBg0"/>
					<BottomLeftColor ref="c_buttonHoveredBg1"/>
					<BottomRightColor ref="c_buttonHoveredBg1"/>
				</GradientBackground>
				<GradientBackground label="Pressed#Pressed" enabled="false">
					<TopLeftColor ref="c_buttonPressedBg0"/>
					<TopRightColor ref="c_buttonPressedBg0"/>
					<BottomLeftColor ref="c_buttonPressedBg1"/>
					<BottomRightColor ref="c_buttonPressedBg1"/>
				</GradientBackground>
				<PlainBackground label="Active#Disabled" enabled="false">
					<Color ref="c_buttonDisabledBg"/>
				</PlainBackground>
				
			</BackgroundDecorators>
		</Appearance>
	</Button>
	
	
